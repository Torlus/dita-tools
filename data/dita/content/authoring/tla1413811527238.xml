											<row>
												<entry ixia_locid="1172"><b ixia_locid="1174" translate="no">MISSING_MANDATORY_CUSTOMER_INFO</b></entry>
												<entry ixia_locid="1176">
												<p ixia_locid="1181">Des données concernant
												l’acheteur sont manquantes.</p>
												</entry>
